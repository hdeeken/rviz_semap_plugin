<package>
  <description brief="rviz_semap_plugins">
    RViz utilities for the SEMAP project.
  </description>
  <author>hdeeken</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/rviz_semap_plugins</url>
  <depend package="rospy"/>
  <depend package="roscpp"/>

  <depend package="std_msgs"/>
  <depend package="mesh_msgs"/>
  <depend package="geometry_msgs"/>
  <depend package="visualization_msgs"/>

  <depend package="rviz"/>
  <depend package="interactive_markers"/>

  <depend package="lvr_ros"/>
  <depend package="semap"/>
  <depend package="semap_ros"/>
  <depend package="semap_env"/>

  <depend package="python_qt_binding"/>

  <export>
    <rviz plugin="${prefix}/plugin_description.xml"/>
  </export>

</package>


